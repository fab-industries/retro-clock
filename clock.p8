end


function _update()
 t+=1
 
 if mode=="clock" then
  update_clock()
 elseif mode=="stop" then
  update_stop()
 end
 
end


function _draw()
 if mode=="clock" then
  draw_clock()
 elseif mode=="stop" then
  draw_stop()
 end
end
