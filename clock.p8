function update_clock()
 
 if override or t%30 ==0 then
	 utc_d=stat(82)
	 utc_h=stat(83)
	 z_d=stat(92)
	 
	 if dst then
	  z_h=stat(93)+1
	 else
	  z_h=stat(93)
	 end
	 
	 get_offset()
	 
	 h=tostr(z_h)
	 m=tostr(stat(94))
	 s=tostr(stat(95)) 
	 yr=tostr(stat(90))
	 mo=tostr(stat(91))
	 dy=tostr(stat(92))
	 
	 if (#h==1) h="0"..h
	 if (#m==1) m="0"..m
	 if (#s==1) s="0"..s
	 if (#mo==1) mo="0"..mo
	 if (#dy==1) dy="0"..dy
	  
	 h1=tonum(sub(h,1,1))
	 h2=tonum(sub(h,2,2))
	 m1=tonum(sub(m,1,1))
	 m2=tonum(sub(m,2,2))
	 s1=tonum(sub(s,1,1))
	 s2=tonum(sub(s,2,2))
	 
	 if (override) override=false
	 
 end 
  
 if btnp(âŽ) then
  mode="stop"
 end 
 if btnp(â¬†ï¸) then
  if dst==false then
   dst=true
  else
   dst=false
  end
 end 
 
end

function update_stop()
 
 if override or t%30 ==0 then
	 utc_d=stat(82)
	 utc_h=stat(83)
	 z_d=stat(92)
	 z_h=stat(93)
	 
	 get_offset()
	 
	 h=tostr(stat(93))
	 m=tostr(stat(94))
	 s=tostr(stat(95)) 
	 yr=tostr(stat(90))
	 mo=tostr(stat(91))
	 dy=tostr(stat(92))
	 
	 if (#h==1) h="0"..h
	 if (#m==1) m="0"..m
	 if (#s==1) s="0"..s
	 
	 if (#mo==1) mo="0"..mo
	 if (#dy==1) dy="0"..dy
	  
	 h1=tonum(sub(h,1,1))
	 h2=tonum(sub(h,2,2))
	 m1=tonum(sub(m,1,1))
	 m2=tonum(sub(m,2,2))
	 s1=tonum(sub(s,1,1))
	 s2=tonum(sub(s,2,2))
	 
	 if stopmode then
	  local nowtime
	  nowtime=stopwatch.total+(time()-stopwatch.start)
	  stopwatch.now=nowtime
	 end
	 
	 stopm=tostr(flr(stopwatch.now)\60)
	 stoph=tostr(stopm\60)
	 stops=tostr((flr(stopwatch.now))%60) 
	 
	 if (#stoph==1) stoph="0"..stoph
	 if (#stopm==1) stopm="0"..stopm
	 if (#stops==1) stops="0"..stops
	
	 stoph1=tonum(sub(stoph,1,1))
	 stoph2=tonum(sub(stoph,2,2))
	 stopm1=tonum(sub(stopm,1,1))
	 stopm2=tonum(sub(stopm,2,2))
	 stops1=tonum(sub(stops,1,1))
	 stops2=tonum(sub(stops,2,2))
	 
	 if (override) override=false
	 
 end

 if stopmode then
  if btnp(âŽ) then
   stopmode=false
   stopwatch.stop=time()
   stopwatch.int=stopwatch.stop-stopwatch.start
   stopwatch.total+=stopwatch.int
   stopwatch.int=0
   override=true
  end
 else
  if btnp(âŽ) then
   stopmode=true
   stopwatch.start=time()
   override=true
  end
  
 end
 
 if btnp(ðŸ…¾ï¸) then
  stopmode=false
  stopwatch.start=0
  stopwatch.stop=0
  stopwatch.total=0
  stopwatch.now=0
  override=true
 end
 
 if btnp(â¬…ï¸) then
